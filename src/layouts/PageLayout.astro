---
import Layout from '~/layouts/Layout.astro';
import Header from '~/components/common/Header.astro'


import type { MetaData } from '~/types';

export interface Props {
  metadata?: MetaData;
}

const { metadata } = Astro.props;
---

<Layout metadata={metadata}>
  <slot name="header">
    <Header />
  </slot>
  <main>
    <slot />
  </main>

</Layout>
